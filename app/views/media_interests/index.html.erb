<h1>Listing media_interests</h1>

<table>
  <tr>
    <th>User profile</th>
    <th>Name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @media_interests.each do |media_interest| %>
  <tr>
    <td><%= media_interest.user_profile_id %></td>
    <td><%= media_interest.name %></td>
    <td><%= link_to 'Show', media_interest %></td>
    <td><%= link_to 'Edit', edit_media_interest_path(media_interest) %></td>
    <td><%= link_to 'Destroy', media_interest, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

