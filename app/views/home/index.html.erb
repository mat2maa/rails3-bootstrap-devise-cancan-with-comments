<%= gmaps(
        :map_options => {
            detect_location: true,
            center_on_user: true,
            auto_zoom: false,
            zoom: 16,
            type: 'SATELLITE'
        },
        markers: {
            data: @json
        }
    ) %>

<h3>Home</h3>
<% @users.each do |user| %>
  <p>User: <%=link_to user.name, user %></p>
<% end %>
