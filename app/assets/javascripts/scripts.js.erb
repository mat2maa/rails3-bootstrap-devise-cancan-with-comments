var type;

$(document).ready(function() {

  $('body').on("click", "#profile-type-select > li > a", function(e) {
    e.preventDefault();
    $('#profile-type-select').find('li').removeClass('active');
    $(this).closest('li').addClass('active');
    type = $(this).closest('li').attr('data-type');
    $('.dynamic-form').fadeIn('fast');
    $('.dynamic-form-inputs')
        .find('.controls-row').fadeOut('fast')
        .find('.' + type).closest('.controls-row').fadeIn('fast');
  });

  //enable chosen js
  $('.select2').select2();

});